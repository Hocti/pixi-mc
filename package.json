{
	"name": "pixi-mc",
	"version": "3.0.1",
	"description": "read 'Texture Atlas' export from Adobe Animate, with MovieClip function in pixi.js",
	"main": "./dist/pixi-mc.cjs.js",
	"module": "./dist/pixi-mc.esm.js",
	"bundle": "./dist/pixi-mc.js",
	"types": "./dist/pixi-mc.d.ts",
	"namespace": "piximc",
	"exports": {
		".": {
			"import": {
				"types": "./dist/pixi-mc.d.ts",
				"default": "./dist/pixi-mc.esm.js"
			},
			"require": {
				"types": "./dist/pixi-mc.d.ts",
				"default": "./dist/pixi-mc.cjs.js"
			}
		}
	},
	"author": "hocti",
	"license": "MIT",
	"keywords": [
		"pixi.js",
		"movieclip",
		"flash",
		"adobe animate",
		"Texture Atlas"
	],
	"readme": "README.md",
	"bugs": "https://github.com/Hocti/piximc/issues",
	"scripts": {
		"build": "rollup -c",
		"watch": "cross-env NODE_ENV=DEV rollup -c --watch",
		"build:prod": "cross-env NODE_ENV=production rollup -c"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Hocti/piximc/"
	},
	"files": [
		"./dist/",
		"tools/",
		"LICENSE",
		"package.json",
		"README.md"
	],
	"peerDependencies": {
		"@pixi/sound": "^6.0.1",
		"pixi-filters": "^6.0.0",
		"pixi.js": "^8.9.1"
	},
	"devDependencies": {
		"@pixi/sound": "^6.0.1",
		"@rollup/plugin-alias": "^5.1.0",
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-json": "^6.0.1",
		"@rollup/plugin-node-resolve": "^16.0.1",
		"@types/node": "^18.6.3",
		"cross-env": "^7.0.3",
		"esbuild": "^0.25.1",
		"eventemitter3": "^5.0.1",
		"pixi-filters": "^6.1.2",
		"pixi.js": "^8.9.1",
		"rollup": "^4.37.0",
		"rollup-plugin-dts": "^6.1.0",
		"rollup-plugin-esbuild": "^6.1.0",
		"rollup-plugin-string": "^3.0.0",
		"typescript": "^5.8.2"
	}
}
